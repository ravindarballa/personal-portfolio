{
  "name": "shell",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "shell",
    "targets": {
      "build": {
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "compiler": "babel",
          "outputPath": "shell/dist",
          "index": "shell/src/index.html",
          "baseHref": "/",
          "main": "shell/src/main.ts",
          "tsConfig": "shell/tsconfig.app.json",
          "assets": [
            "shell/src/favicon.ico",
            "shell/src/assets"
          ],
          "styles": [
            "shell/src/styles.css"
          ],
          "scripts": [],
          "webpackConfig": "shell/webpack.config.ts"
        },
        "configurations": {
          "development": {
            "extractLicenses": false,
            "optimization": false,
            "sourceMap": true,
            "vendorChunk": true
          },
          "production": {
            "fileReplacements": [
              {
                "replace": "shell/src/environments/environment.ts",
                "with": "shell/src/environments/environment.prod.ts"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "webpackConfig": "shell/webpack.config.prod.ts"
          }
        },
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      },
      "serve": {
        "executor": "@nx/react:module-federation-dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "shell:build",
          "hmr": true,
          "port": 4300
        },
        "configurations": {
          "development": {
            "buildTarget": "shell:build:development"
          },
          "production": {
            "buildTarget": "shell:build:production",
            "hmr": false
          }
        },
        "dependsOn": [
          "typecheck"
        ]
      },
      "serve-static": {
        "executor": "@nx/react:module-federation-static-server",
        "defaultConfiguration": "production",
        "options": {
          "serveTarget": "shell:serve"
        },
        "configurations": {
          "development": {
            "serveTarget": "shell:serve:development"
          },
          "production": {
            "serveTarget": "shell:serve:production"
          }
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "shell/jest.config.cts"
        }
      }
    }
  },
  "devDependencies": {
    "aboutme": "workspace:*"
  }
}
